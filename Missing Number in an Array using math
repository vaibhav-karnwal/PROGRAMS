import java.io.*;
import java.util.*;

class Main{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        n+=1;
        int arr[] = new int[n];
        int sum=0;
        sum = n*(n+1)/2;
        for(int i=0;i<n-1;i++){
            arr[i]=scan.nextInt();
            sum-=arr[i];
        }
        System.out.print(sum);
    }
}
