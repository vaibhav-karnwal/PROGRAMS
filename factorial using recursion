import java.io.*;
import java.util.*;

class Main{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        System.out.print(fact(n));
    }

    static int fact(int n){
        if(n==1)return 1;
        return n*fact(n-1);
    }
}
